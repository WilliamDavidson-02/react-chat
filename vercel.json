{
  "routes": [
    {
      "source": "/server/(.*)",
      "destination": "/server/server.js",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    },
    {
      "source": "/server/(.*)",
      "destination": "/server/server.js"
    }
  ]
}
